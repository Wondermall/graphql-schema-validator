query Stores {
  job {
    guides(includeHidden: false) {
        pageInfo {
            hasPreviousPage
            hasNextPage
            startCursor
            endCursor
        }
        edges {
            cursor,
            node {
                id
                title
                description
                imageUrl
                priority
                startDate
                segments
                isHidden
                createdAt
                updatedAt
                hiddenAt
            }
        }
    }
  }
}
